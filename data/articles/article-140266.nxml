      <ref-list id="article-140266.s17">
        <title>References</title>
        <ref id="article-140266.r1">
          <label>1</label>
          <element-citation publication-type="journal">
            <person-group>
              <name>
                <surname>Robertson</surname>
                <given-names>DM</given-names>
              </name>
