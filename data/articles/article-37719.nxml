      <sec id="article-37719.s11" sec-type="Complications">
        <title>Complications</title>
        <list list-type="bullet">
          <list-item>
            <p>Respiratory Arrest</p>
          </list-item>
          <list-item>
            <p>Apnea</p>
          </list-item>
          <list-item>
