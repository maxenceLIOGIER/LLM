      <sec id="article-30385.s11" sec-type="Complications">
        <title>Complications</title>
        <list list-type="bullet">
          <list-item>
            <p>Arrhythmias</p>
          </list-item>
          <list-item>
            <p>Cardiac arrest</p>
          </list-item>
          <list-item>
            <p>Acute lung injury</p>
          </list-item>
          <list-item>
